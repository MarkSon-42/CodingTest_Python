    "def fib_navie(n):\n",
    "    if n == 0:\n",
    "        return 0\n",
    "    elif n == 1:\n",
    "        return 1\n",
    "    else:\n",
    "        fib = fib_navie(n - 1) + fib_navie(n - 2)\n",
    "        return fib"
   ]
